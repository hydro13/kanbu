#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cross-env@10.1.0/node_modules/cross-env/dist/bin/cross-env.js" "$@"
else
  exec node  "$basedir/../../../../../cross-env@10.1.0/node_modules/cross-env/dist/bin/cross-env.js" "$@"
fi
